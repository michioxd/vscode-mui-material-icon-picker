{
    "compilerOptions": {
        "jsx": "react-jsx",
        "jsxImportSource": "preact",
        "composite": true,
        "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo",
        "skipLibCheck": true,
        "module": "ESNext",
        "moduleResolution": "bundler",
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "noEmit": true,
        "baseUrl": "./",
        "paths": {
            "react": [
                "./node_modules/preact/compat/"
            ],
            "react-dom": [
                "./node_modules/preact/compat/"
            ]
        }
    }
}